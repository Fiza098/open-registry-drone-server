{
  "name": "@chronicled/open-registry-server",
  "version": "0.0.0",
  "description": "Server for the Open Registry SDK",
  "main": "index.js",
  "scripts": {
    "start": "sudo node ./server.js",
    "test": "./node_modules/.bin/eslint --ignore-path .gitignore ./"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chronicled/open-registry-server.git"
  },
  "keywords": [],
  "author": "Chronicled Devs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chronicled/open-registry-server/issues"
  },
  "homepage": "https://github.com/chronicled/open-registry-server",
  "devDependencies": {
    "eslint": "2.13.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "eol-last": 2
    }
  },
  "dependencies": {
    "autoprefixer": "6.5.0",
    "axios": "0.14.0",
    "babel": "6.5.2",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-react": "6.16.0",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "express": "4.14.0",
    "forever": "0.15.2",
    "json-loader": "0.5.4",
    "lodash": "4.16.3",
    "morgan": "1.7.0",
    "open-registry-sdk": "0.0.34",
    "postcss-loader": "0.13.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-tap-event-plugin": "1.0.0",
    "react-toggle": "2.1.1",
    "style-loader": "0.13.1",
    "webpack": "1.13.2"
  }
}
